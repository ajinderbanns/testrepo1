{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@hooks/*": ["src/hooks/*"],
      "@utils/*": ["src/utils/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@animations/*": ["src/animations/*"],
      "@visualizations/*": ["src/visualizations/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
